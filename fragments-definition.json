{
    "taskDefinitionArn": "arn:aws:ecs:us-east-1:471269019427:task-definition/fragments-task:1",
    "containerDefinitions": [
        {
            "name": "fragments-container",
            "image": "471269019427.dkr.ecr.us-east-1.amazonaws.com/fragments:latest",
            "cpu": 256,
            "memoryReservation": 512,
            "portMappings": [
                {
                    "name": "fragments-container-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": ${{secrets.TASK_PROTOCOL}},
                    "appProtocol": ${{secrets.TASK_APPPROTOCOL}}
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "AWS_COGNITO_POOL_ID",
                    "value":  ${{secrets.TASK_AWS_COGNITO_POOL_ID}}
                },
                {
                    "name": "PORT",
                    "value":  ${{secrets.TASK_PORT}}
                },
                {
                    "name": "LOCATION_URL",
                    "value":  ${{secrets.TASK_LOCATION_URL}}
                },
                {
                    "name": "LOG_LEVEL",
                    "value":  ${{secrets.TASK_LOG_LEVEL}}
                },
                {
                    "name": "AWS_COGNITO_CLIENT_ID",
                    "value":  ${{secrets.TASK_AWS_COGNITO_CLIENT_ID}}
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group":  ${{secrets.TASK_AWSLOGS_GROUPS}},
                    "awslogs-create-group":  ${{secrets.TASK_AWS_LOGS_CREATE_GROUP}},
                    "awslogs-region": ${{secrets.TASK_AWSLOGS_REGION}},
                    "awslogs-stream-prefix": ${{secrets.TASK_AWSLOGS_STREAM_PREFIX}}
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "family": "fragments-task",
    "taskRoleArn": ${{secrets.TASK_TASKROLEARN}},
    "executionRoleArn": ${{secrets.TASK_EXECUTIONROLEARN}},
    "networkMode": "awsvpc",
    "revision": 1,
    "volumes": [],
    "status": "ACTIVE",
    "requiresAttributes": [
        {
            "name": "com.amazonaws.ecs.capability.logging-driver.awslogs"
        },
        {
            "name": "ecs.capability.execution-role-awslogs"
        },
        {
            "name": "com.amazonaws.ecs.capability.ecr-auth"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.19"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.21"
        },
        {
            "name": "com.amazonaws.ecs.capability.task-iam-role"
        },
        {
            "name": "ecs.capability.execution-role-ecr-pull"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.18"
        },
        {
            "name": "ecs.capability.task-eni"
        },
        {
            "name": "com.amazonaws.ecs.capability.docker-remote-api.1.29"
        }
    ],
    "placementConstraints": [],
    "compatibilities": [
        "EC2",
        "FARGATE"
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "registeredAt": ${{secrets.TASK_REGISTEREDAT}},
    "registeredBy": ${{secrets.TASK_REGISTEREDBY}},
    "tags": []
}
